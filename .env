# Base environment defaults for docs-islands
# Override in .env.[mode] or .env.[mode].local
#
# Available override variables:
#   RELEASE=1                        Enable release mode (suppress debug, generate LLMS text)
#   DOCS_ISLANDS_SILENCE_LOG=true    Suppress info/success logs
#   DOCS_ISLANDS_DEBUG=true          Force enable debug logs

# Logging
DOCS_ISLANDS_SILENCE_LOG=false
DOCS_ISLANDS_DEBUG=false

# Build output
DOCS_ISLANDS_SOURCEMAP=true
DOCS_ISLANDS_MINIFY=false
