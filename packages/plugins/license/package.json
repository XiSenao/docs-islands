{
  "name": "@docs-islands/plugin-license",
  "private": true,
  "type": "module",
  "scripts": {
    "build": "rolldown --config rolldown.config.ts",
    "build:watch": "rolldown --config rolldown.config.ts --watch",
    "typecheck": "tsc --noEmit",
    "lint": "eslint . --config ./eslint.config.mjs --fix --cache",
    "clean": "del-cli dist .eslintcache node_modules"
  },
  "devDependencies": {
    "eslint": "catalog:lint",
    "@docs-islands/eslint-config": "workspace:*",
    "rolldown": "catalog:dev",
    "rolldown-plugin-dts": "catalog:dev",
    "rollup-plugin-license": "catalog:dev",
    "picocolors": "catalog:dev",
    "del-cli": "catalog:dev",
    "typescript": "catalog:dev"
  },
  "peerDependencies": {
    "rolldown": "catalog:dev"
  },
  "peerDependenciesMeta": {
    "rolldown": {
      "optional": false
    }
  },
  "exports": {
    ".": {
      "import": "./dist/index.mjs",
      "types": "./dist/index.d.ts"
    }
  }
}
