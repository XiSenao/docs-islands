{
  "name": "@docs-islands/vitepress-docs",
  "private": true,
  "type": "module",
  "scripts": {
    "docs:dev": "vitepress dev",
    "docs:build": "wait-on -d 100 ../dist/node/index.js && vitepress build",
    "docs:preview": "vitepress preview",
    "typecheck": "tsc --noEmit",
    "lint": "eslint . --config ./eslint.config.mjs --fix --cache",
    "link:dev": "del-cli ../dist --force && npm pkg set devDependencies.@docs-islands/vitepress='link:../' && pnpm install && pnpm set:default-link",
    "link:prod": "del-cli ../dist --force && pnpm set:default-link && pnpm --filter @docs-islands/vitepress build && pnpm install",
    "set:default-link": "npm pkg set devDependencies.@docs-islands/vitepress='link:../dist'",
    "clean": "del-cli node_modules .vitepress/dist .vitepress/cache .eslintcache"
  },
  "packageManager": "pnpm@10.7.0",
  "dependencies": {
    "react": "catalog:dev",
    "react-dom": "catalog:dev",
    "vue": "catalog:prod"
  },
  "devDependencies": {
    "eslint": "catalog:lint",
    "@docs-islands/eslint-config": "workspace:*",
    "typescript": "catalog:dev",
    "vitepress": "catalog:frameworks",
    "vitepress-plugin-group-icons": "catalog:docs-dev",
    "vitepress-plugin-llms": "catalog:docs-dev",
    "@nolebase/vitepress-plugin-enhanced-mark": "catalog:docs-dev",
    "@docs-islands/vitepress": "link:../dist",
    "@vitejs/plugin-react-swc": "catalog:dev",
    "tsx": "catalog:dev",
    "pathe": "catalog:dev",
    "wait-on": "catalog:dev",
    "del-cli": "catalog:dev"
  }
}
